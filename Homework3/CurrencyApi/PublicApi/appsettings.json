{
  "Serilog": {
	"MinimumLevel": {
	  "Default": "Debug",
	  "Override": {
		"System": "Error",
		"Microsoft": "Error",
		"Microsoft.Hosting": "Information"
	  }
	},
	"Properties": {
	  "Site": "Website"
	},
	"WriteTo:Console": {
	  "Name": "Console",
	  "Args": {
		"outputTemplate": "{Timestamp:dd.MM.yy HH:mm:ss.fff} [{Level:u3}] {Message:lj} Properties: {Properties}{NewLine}{Exception}"
	  }
	},

	"Logging": {
	  "LogLevel": {
		"Default": "Information",
		"Microsoft.AspNetCore": "Warning"
	  }
	},
	"AllowedHosts": "*"
  }
}